<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>SaidOut.Security</RootNamespace>
    <TargetFrameworks>netstandard2.0;netstandard1.6;net47;net462;net461;net46;net452;net451;net45</TargetFrameworks>
    <Authors>Said Outgajjouft</Authors>
    <Description>Contain classes that generate random data/value using a cryptographic random number generator.</Description>
    <Copyright>Said Outgajjouft</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/saidout/saidout-security-securerandom/raw/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/saidout/saidout-security-securerandom</PackageProjectUrl>
    <PackageIconUrl>https://github.com/saidout/saidout-security-securerandom/raw/master/saidout_icon32x32.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/saidout/saidout-security-securerandom</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>random data; random number; random value; secure random number generator</PackageTags>
  </PropertyGroup>

</Project>
